    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cr;hasCountryTag">
        <rdfs:label>has country tag</rdfs:label>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;CountryIdentifier"/>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasUniqueIdentifier"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cr;hasEnglishFullName">
        <rdfs:label>has English full name</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">The full name, if different from the short form of the country name, in lower case; the formal title as notified by the country concerned to the UN Secretary General.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-lng-lr;hasEnglishName"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cr;hasEnglishShortName">
        <rdfs:label>has English short name</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the short form of the country name, in English</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-lng-lr;hasEnglishName"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cr;hasEnglishShortNameInCapitals">
        <rdfs:label>has English short name in capitals</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the short form of the country name, in English (capitals)</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-lng-lr;hasEnglishName"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cr;hasFrenchFullName">
        <rdfs:label>has French full name</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-lng-lr;hasFrenchName"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cr;hasFrenchShortName">
        <rdfs:label>has French short name</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-lng-lr;hasFrenchName"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cr;hasFrenchShortNameInCapitals">
        <rdfs:label>has French short name in capitals</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-lng-lr;hasFrenchName"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cr;hasLocalShortName">
        <rdfs:label>has local short name</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the short form of the country name according to the common use</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-aap-agt;hasName"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cr;hasNumericCountryCode">
        <rdfs:label>has numeric country code</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">Also called numeric-3 code in the specification, the three-digit numeric code representing the country name.

Recognizing that a numeric code for country names is of advantage (e.g. to provide language independence), a three-digit numeric (numeric-3) code from the range 000 to 899 is also provided in this part of IS0 3166.  It is made available by the United Nations Statistics Division.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;hasUniqueIdentifier"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cr;hasRemarks">
        <rdfs:label>has remarks</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">Remarks, such as other widely-used country names and names of geographically separated territories covered by the main entry in the list(the latter are indexed in Annex A)</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cr;hasSubdivisionName">
        <rdfs:label>has subdivision name</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-aap-agt;hasName"/>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;CountrySubdivision"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&fibo-fnd-plc-cr;isIndependent">
        <rdfs:label>is independent</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">indicates whether a country is independent from an international political perspective</skos:definition>
        <rdfs:domain rdf:resource="&fibo-fnd-plc-cr;Country"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    

