    <owl:NamedIndividual rdf:about="&fibo-fnd-plc-3166-2;District">
        <rdf:type rdf:resource="&fibo-fnd-plc-cr;SubdivisionKind"/>
        <rdfs:label>district</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a country subdivision that is a type of administrative or territorial division established by a government for a specific purpose that, in some countries, is managed by a local government</skos:definition>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;anyURI">Across the world, areas known as districts vary greatly in size, spanning entire regions or counties, several municipalities, subdivisions of municipalities, school district, or political district.</fibo-fnd-utl-av:explanatoryNote>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.merriam-webster.com/dictionary/district</fibo-fnd-utl-av:adaptedFrom>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/District</fibo-fnd-utl-av:adaptedFrom>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&fibo-fnd-plc-3166-2;OutlyingArea">
        <rdf:type rdf:resource="&fibo-fnd-plc-cr;SubdivisionKind"/>
        <rdfs:label>outlying area</rdfs:label>
        <skos:prefLabel>outlying area</skos:prefLabel>
        <skos:altLabel>insular area</skos:altLabel>
        <skos:definition rdf:datatype="&xsd;string">a country subdivision that is a type of administrative or territorial division that is typically remote from the main geopolitical entity</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/Insular_area</fibo-fnd-utl-av:adaptedFrom>
    </owl:NamedIndividual>

